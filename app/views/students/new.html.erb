<h1>New Student Form</h1>

<%=form_tag students_path do%>
  <label>first_name:</label><br>
  <%=text_field_tag :'student[first_name]'%><br>

  <label>Last Name:</label><br>
  <%=text_field_tag :'student[last_name]'%><br>

  <label>Date Created:</label><br>
  <%=text_field_tag :'student[created_at]'%><br>

  <label>Date Updated:</label><br>
  <%=text_field_tag :'student[updated_at]'%><br>

  <%= submit_tag :"Submit Student"%>
<%end%>
